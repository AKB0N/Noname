<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TintViewDrawable">
        <attr name="drawableTint" format="color"/>
    </declare-styleable>

    <declare-styleable name="MaterialLoadingView">
        <attr name="ring_color" format="color" />
        <attr name="style">
            <enum name="Default" value="0" />
            <enum name="Large" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ResizingText">
        <attr name="resizing_text_min_size" format="dimension"/>
    </declare-styleable>
</resources>